//********** Includes *************************************************************

//********** Variables ************************************************************
const int zumba=12;

//********** Setup ****************************************************************
void setup()
{
  pinMode(zumba,OUTPUT);
}

//********** Loop *****************************************************************
void loop()
{
  digitalWrite(12,HIGH);
  delay(1000);
  digitalWrite(12,LOW);
  delay(1000);
}

